# Application settings
APP_NAME="LARP Admin API"
APP_VERSION="0.1.0"
DEBUG=true
ENVIRONMENT=development

# Database (Supabase PostgreSQL via pooler)
DATABASE_URL=postgresql+asyncpg://postgres.your-tenant-id:your-super-secret-and-long-postgres-password@localhost:6543/postgres
DATABASE_ECHO=false

# CORS - comma-separated origins
CORS_ORIGINS=["http://localhost:3000", "http://localhost:5173"]

# Pagination
DEFAULT_PAGE_SIZE=20
MAX_PAGE_SIZE=100
